  <%= form_with(model: project, local: true) do |form| %>

    <div>
      <%= form.label :Project_name, style: "display: block" %>
      <%= form.text_field :name,required: true %>
    </div>

    <div>
      <%= form.label :Member %>
      <%= form.select :member_id, members ,prompt: 'please select member' %>
    </div>

    <div>
      <%= form.submit %>
    </div>
<% end%>
